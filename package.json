{
  "name": "profile-zesova",
  "version": "1.0.0",
  "description": "Profile of Venkat, You can find Venkat at https://github.com/Venkataramanan21",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:sass": "node-sass sass/main.scss css/styles.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "compress:css": "node-sass css/style.concat.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css compress:css",
    "build:parcel": "parcel build index.html",
    "build": "npm-run-all build:css build:parcel"
  },
  "author": "Venkataramanan B",
  "license": "ISC",
  "devDependencies": {
    "concat": "^1.0.3",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.2",
    "postcss": "^8.4.39"
  }
}
